{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/next" }],
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "silent": false,
    "deployments": {
      "productionBranch": "master"
    }
  },
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "codeaprogram.tech" }],
      "destination": "https://www.codeaprogram.tech/$1",
      "permanent": true
    }
  ]
}
